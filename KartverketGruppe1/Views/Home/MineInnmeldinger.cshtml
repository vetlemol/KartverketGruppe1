           }

           /* Stiler for søkefelt og knapper */
           .search-container {
               display: flex;
               align-items: center;
               justify-content: flex-start;
               margin-left: 70px;
               margin-top: 10px;
           }

               .search-container input[type="text"] {
                   padding: 10px;
                   font-size: 12px;
                   border: 1px solid #ccc;
                   border-radius: 37px;
                   width: 200px;
                   margin-right: 10px;
               }

               /* Dropdown stil */
               .search-container select {
                   padding: 10px;
                   font-size: 12px;
                   border: 1px solid #ccc;
                   border-radius: 37px;
                   margin-right: 10px;
                   background-color: white;
                   cursor: pointer;
               }

           .blue-box {
               background-color: #D8E3F2;
               color: black;
               padding: 20px;
               width: 1000px;
               height: auto;
               border-radius: 15px;
               text-align: center;
               margin-left: 70px;
               margin-top: 23px;
           }

           /* Boksene inne i den store blå boksen */
           .item-box {
               display: flex;
               justify-content: space-between;
               align-items: center;
               background-color: white;
               border-radius: 10px;
               padding: 15px;
               margin-bottom: 10px;
               border: 1px solid #ccc;
           }

               .item-box .info {

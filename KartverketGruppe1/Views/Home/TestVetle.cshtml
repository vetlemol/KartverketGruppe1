@{
}
@{
    ViewData["Title"] = "Lag Bruker Ny";
}

<h2>Lag Bruker</h2>

<form asp-action="TestVetle" method="post">

    <label for="Fornavn">Fornavn:</label>
    <input type="text" id="Fornavn" name="Fornavn" required>
    <br>
    <label for="Etternavn">Etternavn:</label>
    <input type="text" id="Etternavn" name="Etternavn" required>
    <br>
    <label for="Epost">Epost:</label>
    <input type="email" id="Epost" name="Epost" required>
    <br>
    <label for="Ansvarsområde">Ansvarsområde:</label>
    <input type="text" id="Ansvarsområde" name="Ansvarsområde" required>
    <br>
    <label for="Avdeling">Avdeling:</label>
    <input type="text" id="Avdeling" name="Avdeling" required>
    <br>
    <label for="Passord">Passord:</label>
    <input type="password" id="Passord" name="Passord" required>
    <br>
    <br />
    <button type="submit" class="btn btn-primary">Submit</button>
</form>